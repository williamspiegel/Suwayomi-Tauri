{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Suwayomi Launcher",
  "version": "0.1.0",
  "identifier": "org.suwayomi.launcher",
  "build": {},
  "bundle": {
    "active": true,
    "targets": "all",
    "resources": [
      "resources"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Suwayomi",
        "width": 1280,
        "height": 800,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  }
}
